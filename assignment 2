"Q1"
n=int(input("Enter a number"))
for i in range(1,n+1):
    print(i,end=" ")
print()
for i in range(n,0,-1):
    print(i,end=" ")

"Q2"
for i in range(1,101):
    if i%2==0:
        print(i,end=" ")
    else:
        print()
        print(i,end="  ")
        
"Q3"
n=int(input("Enter a number:"))
count=0
for i in range(1,n+1):
    if(n%i==0):
        count=count+1
if(count>2):
    print(n," Not prime")
else:
    print(n," is prime")

"Q4"
n=int(input("Enter a number:"))
sum=0
for i in range(1,n):
    if i%3==0 or i%5==0:
        print(i,end=" ")
        sum=sum+i
print()
print("sum =",sum)

"Q5"
def fact(number):
    factorial = 1
    while(number != 1):
        factorial = factorial*number
        number -=1
    return factorial
def calculate(num, choice):
    if choice == "sum":
        print(f"Sum(1+2+3+....+{num}) = {sum(range(1,n+1))}")
    else:
        print(f"Product(1.2.3...{num}) = {fact(num)}")


n = int(input("Enter a number:"))
choice = input(f"Enter sum(1+2+3+....+{n}) or product(1.2.3...{n}) : sum/product:")
calculate(n, choice)

